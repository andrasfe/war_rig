    },
    {
      "rule_id": "BR003",
      "description": "US Phone: 3-digit area (valid NA code, non-zero), 3-digit prefix (non-zero), 4-digit line (non-zero), optional overall",
      "logic_summary": "Separate NUMERIC checks per part, lookup VALID-GENERAL-PURP-CODE for area",
      "conditions": [
        "NOT NUMERIC",
        "==0",
        "NOT VALID-GENERAL-PURP-CODE"
      ],
      "citation": [
        2246,
        2316,
        2370,
        2296
