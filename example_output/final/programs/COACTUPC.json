    {
      "rule_id": "BR002",
      "description": "Names (first, last) must contain only alphabets and spaces, required except middle optional",
      "logic_summary": "INSPECT CONVERTING alpha chars to spaces, check TRIM length ==0 after",
      "conditions": [
        "FLG-ALPHA-NOT-OK",
        "FLG-ALPHA-BLANK"
      ],
      "citation": [
        1925,
        1930,
        1563,
        1579
