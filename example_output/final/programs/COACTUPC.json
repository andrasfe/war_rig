  "business_rules": [
    {
      "rule_id": "BR001",
      "description": "Account ID must be exactly 11-digit non-zero numeric",
      "logic_summary": "TEST-NUMVAL-C and check != ZEROS after move to numeric",
      "conditions": [
        "CC-ACCT-ID IS NOT NUMERIC",
        "CC-ACCT-ID-N EQUAL ZEROS"
